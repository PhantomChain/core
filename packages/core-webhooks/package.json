{
  "name": "@phantomchain/core-webhooks",
  "description": "Webhooks for PHANTOM Core",
  "version": "0.1.4",
  "contributors": [
    "Brian Faust <brian@phantom.org>"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build:docs": "../../node_modules/.bin/jsdoc -c jsdoc.json",
    "test": "PHANTOM_ENV=test jest --forceExit",
    "test:coverage": "PHANTOM_ENV=test jest --coverage --forceExit",
    "test:debug": "PHANTOM_ENV=test node --inspect-brk ../../node_modules/.bin/jest --runInBand --watch",
    "test:watch": "PHANTOM_ENV=test jest --runInBand --watch",
    "test:watch:all": "PHANTOM_ENV=test jest --runInBand --watchAll",
    "flow:coverage": "flow-coverage-report --config ../../.flowcoverage.json",
    "flow:check": "flow check ./lib",
    "flow:deps": "flow-typed install",
    "lint": "eslint ./ --fix",
    "depcheck": "depcheck ./ --ignores=flow-bin,sqlite3"
  },
  "dependencies": {
    "@phantomchain/core-container": "^0.1.6",
    "@phantomchain/core-http-utils": "^0.1.4",
    "axios": "^0.18.0",
    "boom": "^7.2.2",
    "fs-extra": "^7.0.1",
    "hapi-pagination": "^2.0.1",
    "joi": "^14.0.4",
    "sequelize": "^4.41.1",
    "sqlite3": "^4.0.4",
    "umzug": "^2.1.0"
  },
  "devDependencies": {
    "@phantomchain/core-test-utils": "^0.1.7",
    "flow-bin": "^0.86.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.x"
  }
}
