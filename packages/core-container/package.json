{
  "name": "@phantomchain/core-container",
  "description": "Container for PHANTOM Core",
  "version": "0.1.6",
  "contributors": [
    "Brian Faust <brian@ark.io>"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build:docs": "../../node_modules/.bin/jsdoc -c jsdoc.json",
    "test": "PHANTOM_ENV=test jest",
    "test:coverage": "PHANTOM_ENV=test jest --coverage",
    "test:debug": "PHANTOM_ENV=test node --inspect-brk ../../node_modules/.bin/jest --runInBand --watch",
    "test:watch": "PHANTOM_ENV=test jest --runInBand --watch",
    "test:watch:all": "PHANTOM_ENV=test jest --runInBand --watchAll",
    "flow:coverage": "flow-coverage-report --config ../../.flowcoverage.json",
    "flow:check": "flow check ./lib",
    "flow:deps": "flow-typed install",
    "lint": "eslint ./ --fix",
    "depcheck": "depcheck ./ --ignores=flow-bin"
  },
  "dependencies": {
    "@phantomchain/crypto": "^0.2.9",
    "awilix": "^4.0.0",
    "axios": "^0.18.0",
    "delay": "^4.1.0",
    "envfile": "^2.3.0",
    "expand-home-dir": "^0.0.3",
    "fs-extra": "^7.0.1",
    "hoek": "^6.0.3",
    "lodash": "^4.17.11",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.15.0",
    "flow-bin": "^0.86.0",
    "jest-mock-process": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.x"
  }
}
